{
  "name": "@completium/archetype-binder-ts",
  "version": "0.1.15",
  "description": "Binds Archetype contract interface to Typescript",
  "main": "build/src/main.js",
  "types": "build/src/main.d.ts",
  "files": [
    "/build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "generate": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/generate.ts",
    "test:run": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/*.spec.ts",
    "test:fa1": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/fa1.spec.ts",
    "test:fa2": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/fa2_fungible.spec.ts",
    "test:multisig": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/multisig.spec.ts",
    "test:path": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/path.spec.ts",
    "test:test": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/test.spec.ts",
    "test:training": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/training.spec.ts",
    "test:template": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/template.spec.ts",
    "test:type_asset_only_key_1": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_asset_only_key_1.spec.ts",
    "test:type_asset_only_key_2": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_asset_only_key_2.spec.ts",
    "test:type_asset_only_key_1_big_map": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_asset_only_key_1_big_map.spec.ts",
    "test:type_asset_only_key_2_big_map": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_asset_only_key_2_big_map.spec.ts",
    "test:type_asset_key_1": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_asset_key_1.spec.ts",
    "test:type_asset_key_2": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_asset_key_2.spec.ts",
    "test:type_asset_value_2": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_asset_value_2.spec.ts",
    "test:type_asset_value_3": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_asset_value_3.spec.ts",
    "test:type_asset_key_1_big_map": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_asset_key_1_big_map.spec.ts",
    "test:type_asset_key_2_big_map": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_asset_key_2_big_map.spec.ts",
    "test:type_asset_value_2_big_map": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_asset_value_2_big_map.spec.ts",
    "test:type_asset_value_3_big_map": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_asset_value_3_big_map.spec.ts",
    "test:type_asset_key_1_iterable_big_map": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_asset_key_1_iterable_big_map.spec.ts",
    "test:type_asset_key_2_iterable_big_map": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_asset_key_2_iterable_big_map.spec.ts",
    "test:type_asset_value_2_iterable_big_map": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_asset_value_2_iterable_big_map.spec.ts",
    "test:type_asset_value_3_iterable_big_map": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_asset_value_3_iterable_big_map.spec.ts",
    "test:type_big_map_key": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_big_map_key.spec.ts",
    "test:type_big_map_value": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_big_map_value.spec.ts",
    "test:type_fails": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_fails.spec.ts",
    "test:type_getter": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_getter.spec.ts",
    "test:type_list": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_list.spec.ts",
    "test:type_map_key": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_map_key.spec.ts",
    "test:type_map_value": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_map_value.spec.ts",
    "test:type_option": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_option.spec.ts",
    "test:type_or_left": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_or_left.spec.ts",
    "test:type_or_right": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_or_right.spec.ts",
    "test:type_parameter": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_parameter.spec.ts",
    "test:type_record": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_record.spec.ts",
    "test:type_set": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_set.spec.ts",
    "test:type_simple": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_simple.spec.ts",
    "test:type_tuple": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_tuple.spec.ts",
    "test:type_view": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/type_view.spec.ts",
    "test:sample": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/sample.spec.ts",
    "test:asset_view": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/asset_view.spec.ts",
    "test:asset_aggregate_partition": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/asset_aggregate_partition.spec.ts",
    "test:lambda": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/lambda.spec.ts",
    "test:ticket": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/ticket.spec.ts",
    "test:event": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/event.spec.ts",
    "test:state": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/state.spec.ts",
    "test:michelson": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/michelson.spec.ts",
    "test:utils": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/utils.spec.ts",
    "test:generate:coverage": "nyc npm run test:generate",
    "test:run:coverage": "nyc npm run test:run",
    "build": "rm -rf ./build && npx tsc --outDir build",
    "exec": "rm -rf ./build && npx tsc --project tsconfig.json && node ./build/src/main.js",
    "unit": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/unit.ts",
    "unit:passed": "ts-mocha --timeout 0 --slow 99999999999999999 ./tests/unit_passed.ts",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
    "init": "completium-cli init",
    "mockup_init": "completium-cli mockup init"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/completium/archetype-binder-ts.git"
  },
  "keywords": [
    "tezos",
    "archetype",
    "typescript",
    "test"
  ],
  "author": "Completium",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/completium/archetype-binder-ts/issues"
  },
  "homepage": "https://github.com/completium/archetype-binder-ts#readme",
  "dependencies": {
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@completium/archetype": "1.0.0",
    "@completium/archetype-ts-types": "0.1.12",
    "@completium/completium-cli": "1.0.0",
    "@completium/event-listener": "^0.0.5",
    "@completium/experiment-ts": "0.1.13",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "nyc": "^15.1.0",
    "ts-mocha": "^10.0.0"
  },
  "nyc": {
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/!(*.test.*).[tj]s?(x)"
    ],
    "exclude": [
      "src/_tests_/**/*.*"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage"
  }
}
